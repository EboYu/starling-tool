// test variable
shared int test;

// Junk variables
shared int foo;
shared int bar;

view isZero();
view isThree();

method addThree() {
  {| isZero() |}
  <| test++;
     test++;
     test++; |>
  {| isThree() |}
}

constraint isZero() -> test==0;
constraint isThree() -> test==3;

// No-one else is relying on test staying zero
constraint isZero() * isZero() -> false;
